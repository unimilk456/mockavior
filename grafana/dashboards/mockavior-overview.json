{
  "uid": "mockavior-overview",
  "title": "Mockavior Â· Overview",
  "schemaVersion": 38,
  "version": 7,
  "refresh": "10s",
  "time": { "from": "now-30m", "to": "now" },
  "tags": ["mockavior", "overview"],
  "panels": [
    {
      "type": "timeseries",
      "title": "Routing latency p95",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (increase(mockavior_routing_match_time_seconds_bucket[15m])))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP RPS",
      "gridPos": { "x": 0, "y": 6, "w": 24, "h": 6 },
      "targets": [
        {
          "expr": "increase(mockavior_http_requests_total[1m])",
          "legendFormat": "rps"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "CPU usage",
      "gridPos": { "x": 0, "y": 12, "w": 24, "h": 6 },
      "targets": [
        {
          "expr": "process_cpu_usage",
          "legendFormat": "cpu"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Active snapshot",
      "gridPos": { "x": 0, "y": 18, "w": 24, "h": 6 },
      "targets": [
        {
          "expr": "mockavior_snapshot_active"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Snapshots in flight",
      "gridPos": { "x": 0, "y": 24, "w": 24, "h": 6 },
      "targets": [
        {
          "expr": "mockavior_snapshot_in_flight"
        }
      ]
    }
  ]
}
