{
  "uid": "mockavior-http-runtime",
  "title": "Mockavior Â· HTTP Runtime",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-1h", "to": "now" },
  "tags": ["mockavior", "http"],
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP requests",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "increase(mockavior_http_requests_total[1m])",
          "legendFormat": "requests"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP latency avg",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "increase(mockavior_http_request_duration_seconds_sum[5m]) / increase(mockavior_http_request_duration_seconds_count[5m])",
          "legendFormat": "avg"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Active HTTP requests",
      "gridPos": { "x": 0, "y": 6, "w": 24, "h": 6 },
      "targets": [
        {
          "expr": "http_server_requests_active_seconds_active_count",
          "legendFormat": "active"
        }
      ]
    }
  ]
}
