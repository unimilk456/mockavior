{
  "uid": "mockavior-routing-latency",
  "title": "Mockavior Â· Routing & Latency",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-1h", "to": "now" },
  "tags": ["mockavior", "latency"],
  "panels": [
    {
      "type": "timeseries",
      "title": "Routing latency p50 / p95 / p99",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (increase(mockavior_routing_match_time_seconds_bucket[15m])))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (increase(mockavior_routing_match_time_seconds_bucket[15m])))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (increase(mockavior_routing_match_time_seconds_bucket[15m])))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Routing latency max",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "mockavior_routing_match_time_seconds_max",
          "legendFormat": "max"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Routing buckets",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum by (le) (increase(mockavior_routing_match_time_seconds_bucket[15m]))",
          "legendFormat": "le={{le}}"
        }
      ]
    }
  ]
}
